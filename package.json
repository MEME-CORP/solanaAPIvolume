    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@jup-ag/api": "^6.0.21",
    "@solana/spl-token": "^0.3.8",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/web3.js": "^1.87.6",
    "bip39": "^3.1.0",

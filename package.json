    "test:mainnet-funding": "node scripts/test-mainnet-funding.js",
    "test:mainnet-transfer": "node scripts/test-mainnet-sol-transfer.js",
    "test:mainnet-return": "node scripts/test-mainnet-return-funds.js",
    "mainnet-integration": "node scripts/run-mainnet-integration.js",
    "jupiter:fund": "node scripts/jupiter-volume/fund-wallets.js",
    "jupiter:quote": "node scripts/jupiter-volume/get-quote.js",
    "jupiter:buy": "node scripts/jupiter-volume/buy-tokens.js",
    "jupiter:sell": "node scripts/jupiter-volume/sell-tokens.js",
